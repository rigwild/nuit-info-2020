{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Lancer le programme",
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceFolder}/node_modules/**"
      ],
      "env": {
        "DEBUG": "*"
      },
      "program": "${workspaceFolder}/server/dist/index.js",
      "sourceMaps": true,
      "outFiles": [
        "${workspaceFolder}/server/dist/*.js",
        "!${workspaceFolder}/node_modules/**"
      ]
    }
  ]
}