{
  "compilerOptions": {
    "module": "ESNext",
    "target": "es2019",
    "moduleResolution": "node",
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "alwaysStrict": true,
    "declaration": true,
    "esModuleInterop": true,
    "lib": [
      "ES2019"
    ]
  },
  "include": [
    "./*.ts",
    "./routers/*"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}
